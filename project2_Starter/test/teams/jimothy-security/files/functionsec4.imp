function printHi() {
    print_str("hi");
    return;
}

function main() {
    var x = get_int_s();
    var y = get_int();

    while (y < 5) do {
        if (x == y) then {
            printHi();
        }

        else {
            printHi();
        }

        y = y + 1;
    }

    printHi();
    return;
}
